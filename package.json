{
  "name": "webpack_server_build",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:build-server": "webpack --config webpack.server.js --mode=development --watch",
    "dev:start": "nodemon --delay 5 ./server-build/index.js",
    "prod": "npm-run-all prod:*",
    "prod:stop": "pm2 stop serverBundle",
    "prod:build-server": "webpack --config webpack.server.js --mode=production",
    "prod:start": "pm2 start ./server-build/serverBundle.js"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "docs/*"
    ],
    "delay": "5000"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "body-parser": "^1.18.3",
    "dotenv": "^6.1.0",
    "express": "^4.16.3",
    "moment": "^2.22.2",
    "mysql": "^2.16.0",
    "mysql2": "^1.6.1",
    "nodemon": "^1.7.3",
    "npm-run-all": "^4.1.3",
    "pm2": "^3.2.2",
    "puppeteer": "^1.9.0",
    "sequelize": "^4.39.1",
    "uuid": "^3.3.2",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-node-externals": "^1.7.2"
  }
}
